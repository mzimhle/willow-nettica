	<script type="text/javascript">
	
		function submitForm() {
				
				$('.contact-submit').val('Please wait...');
				
				$.ajax({
					type: "GET",
					url: "default.php",
					data: "ajax=1&enquiry_comments="+$('#enquiry_comments').val()+"&enquiry_number="+$('#enquiry_number').val()+"&enquiry_email="+$('#enquiry_email').val()+"&enquiry_name="+$('#enquiry_name').val(),
					dataType: "json",
					success: function(data){
						
						if(data.error.enquiry_name == 0) { $('.enquiry_name_error').css('color', 'red'); } else { $('.enquiry_name_error').css('color', 'white'); }
						if(data.error.enquiry_email == 0) { $('.enquiry_email_error').css('color', 'red'); } else { $('.enquiry_email_error').css('color', 'white'); }
						if(data.error.enquiry_number == 0) { $('.enquiry_number_error').css('color', 'red'); } else { $('.enquiry_number_error').css('color', 'white'); }
						if(data.error.enquiry_comments == 0) { $('.enquiry_comments_error').css('color', 'red'); } else { $('.enquiry_comments_error').css('color', 'white'); }						

							
						if(data.result == 1) {
							$('.contact_error').html('Please use the form below to contact us. - <span style="color: green; font-weight: bold;"> - Thank you for your enquiry, we willl get back to you as soon as possible.</span>');
						} else {							
							$('.contact_error').html('Please use the form below to contact us. <span style="color: red; font-weight: bold;"> - There are some errors on your form.</span>');
						}						
					}
				});
				
				$('.contact-submit').val('Submit');
				
		}
		
